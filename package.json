{
	"name": "vite-template-redux",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"start": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"test": "vitest",
		"format": "prettier --write .",
		"lint": "eslint .",
		"type-check": "tsc",
		"prepare": "husky install",
		"pre-commit": "lint-staged"
	},
	"dependencies": {
		"@hello-pangea/dnd": "^16.5.0",
		"@netlify/functions": "^2.4.1",
		"@reduxjs/toolkit": "^2.0.1",
		"@tanstack/query-sync-storage-persister": "^5.17.1",
		"@tanstack/react-query": "^5.17.1",
		"@tanstack/react-query-persist-client": "^5.17.1",
		"axios": "^1.6.4",
		"body-scroll-lock": "^4.0.0-beta.0",
		"comlink": "^4.4.1",
		"dayjs": "^1.11.10",
		"diff": "^5.1.0",
		"docx": "^8.5.0",
		"file-saver": "^2.0.5",
		"fuse.js": "^7.0.0",
		"html-to-image": "^1.11.11",
		"html2canvas": "^1.4.1",
		"jspdf": "^2.5.1",
		"lodash": "^4.17.21",
		"lz-string": "^1.5.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^4.0.12",
		"react-hook-form": "^7.49.2",
		"react-hotkeys-hook": "^4.4.3",
		"react-merge-refs": "^2.1.1",
		"react-redux": "^9.0.4",
		"react-router-dom": "^6.21.1",
		"react-secure-storage": "^1.3.2",
		"react-textarea-autosize": "^8.5.3",
		"react-toastify": "^9.1.3",
		"react-tooltip": "^5.25.1",
		"react-use": "^17.4.2",
		"redux-persist": "^6.0.0",
		"styled-components": "^6.1.6",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@tanstack/react-query-devtools": "^5.17.1",
		"@testing-library/dom": "^9.3.3",
		"@testing-library/jest-dom": "^6.2.0",
		"@testing-library/react": "^14.1.2",
		"@testing-library/user-event": "^14.5.2",
		"@types/body-scroll-lock": "^3.1.2",
		"@types/diff": "^5.0.9",
		"@types/file-saver": "^2.0.7",
		"@types/lodash": "^4.14.202",
		"@types/react": "^18.2.46",
		"@types/react-dom": "^18.2.18",
		"@types/uuid": "^9.0.7",
		"@vitejs/plugin-react": "^4.2.1",
		"babel-eslint": "^10.1.0",
		"eslint": "^8.56.0",
		"eslint-plugin-jest-dom": "^5.1.0",
		"eslint-plugin-prettier": "^5.1.2",
		"eslint-plugin-testing-library": "^6.2.0",
		"husky": "^8.0.0",
		"jsdom": "^23.0.1",
		"lint-staged": "^15.2.0",
		"msw": "^2.0.11",
		"prettier": "^3.1.1",
		"typescript": "^5.2.2",
		"user-agent-data-types": "^0.4.2",
		"vite": "^5.0.11",
		"vite-plugin-comlink": "^4.0.2",
		"vitest": "^1.1.3"
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"extends": [
			"plugin:testing-library/react",
			"plugin:jest-dom/recommended"
		],
		"plugins": [
			"prettier",
			"jest-dom",
			"testing-library"
		],
		"rules": {
			"testing-library/no-render-in-setup": "error",
			"testing-library/no-wait-for-empty-callback": "error",
			"testing-library/prefer-explicit-assert": "error",
			"testing-library/prefer-presence-queries": "error",
			"testing-library/prefer-screen-queries": "error",
			"testing-library/prefer-wait-for": "error"
		}
	},
	"eslintConfigComments": {
		"parser-related": "https://stackoverflow.com/a/41190209/5800789",
		"testing-library-related": "https://sapegin.me/til/react/ensuring-react-testing-library-best-practices-with-eslint/"
	},
	"msw": {
		"workerDirectory": ""
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write --ignore-unknown"
		],
		"*.{json,html,md,css}": [
			"prettier --write --ignore-unknown"
		]
	}
}
